{
  "name": "ccpay-bubble",
  "version": "2.0.0",
  "engines": {
    "node": ">=v20.19.1"
  },
  "scripts": {
    "debug": "node --inspect --proxy-server=http://proxyout.reform.hmcts.net:8080 start.js",
    "ng": "ng",
    "start": "node start.js",
    "start:angular-dev": "ng serve --proxy-config proxy.conf.json",
    "start:express-dev": "cross-env NODE_ENV=development nodemon --inspect start.js",
    "start:express-prod": "cross-env IGNORE_CERTS=true nodemon --inspect start.js",
    "build": "ng build && yarn pugit dist/ccpay-bubble/index.html --output  express/mvc/views/index.pug && rm dist/ccpay-bubble/index.html && node ./scripts/replace.js file=express/mvc/views/index.pug searchValue='\\| undefined' newValue='// Todo replace pugit'",
    "postbuild": "yarn replace file=express/mvc/views/index.pug searchValue='\\\"csrfToken\\\"' newValue='csrfToken'",
    "ng:build": "ng build view-payment && ng build fee-register-search && yarn build",
    "test:coverage": "yarn merge-lcov && yarn copy-lcov",
    "fortifyScan": "./src/test/java/gradlew -p src/test/java fortifyScan",
    "merge-lcov": "cross-env lcov-result-merger './coverage/ccpay-bubble/lcov-*.info' './coverage/ccpay-bubble/lcov.info'",
    "copy-lcov": "node ./scripts/copy-lcov.js",
    "test": "yarn lint && yarn test:angular && yarn test:coverage",
    "test:a11y": "cross-env node scripts/pa11y",
    "gulp": "gulp",
    "lint": "yarn ng-lint && yarn eslint",
    "audit": "yarn audit --groups dependencies",
    "ng-lint": "ng lint",
    "eslint": "eslint .",
    "lint:fix": "yarn eslint --fix && yarn ng lint --fix",
    "test:angular": "ng test --watch=false --code-coverage && yarn rename source=./coverage/ccpay-bubble/lcov.info destination=./coverage/ccpay-bubble/lcov-angular.info",
    "test:express": "cross-env NODE_ENV=development nyc --reporter=lcov --reporter=text mocha ./express/**/*.test.js --recursive && yarn rename source=./coverage/ccpay-bubble/lcov.info destination=./coverage/ccpay-bubble/lcov-express.info",
    "rename": "node ./scripts/rename.js",
    "replace": "node ./scripts/replace.js",
    "e2e": "ng e2e",
    "sonar-scan": "sonar-scanner",
    "test:mutation": "./bin/run-mutation-tests.sh",
    "test:stryker": "./node_modules/.bin/stryker run ",
    "test:smoke": "codeceptjs run --config smoke-test/smoke-codecept.conf.js",
    "test:functional": "yarn playwright install && npm-run-all -scl test:functional:pipeline test:functional:report",
    "test:functional-debug": "yarn playwright install && npm-run-all -scl test:functional:debug test:functional:report",
    "test:fullfunctional": "yarn playwright install && npm-run-all -scl test:functional:nightly test:functional:report",
    "test:functional:debug": "codeceptjs run-workers 6 parallel --config acceptance-tests/codecept.conf.js --grep @debug",
    "test:functional:serial": "codeceptjs run --config acceptance-tests/codecept.conf.js --grep @serial",
    "test:functional:pipeline:parallel": "codeceptjs run-workers 6 parallel --config acceptance-tests/codecept.conf.js --grep @pipeline --invert --grep @serial",
    "test:functional:pipeline": "npm-run-all --parallel --continue-on-error test:functional:serial test:functional:pipeline:parallel",
    "test:functional:nightly:parallel": "codeceptjs run-workers 6 parallel --config acceptance-tests/codecept.conf.js --grep @nightly --invert --grep @serial",
    "test:functional:nightly": "npm-run-all --parallel --continue-on-error test:functional:serial test:functional:nightly:parallel",
    "test:functional:report": "allure generate functional-output/functional/reports -c -o functional-output/functional/allure",
    "test:crossbrowser": "yarn playwright install && npm-run-all -scl test:crossbrowser:e2e test:crossbrowser:report",
    "test:crossbrowser:e2e": "yarn playwright install && NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run-multiple --all --config acceptance-tests/saucelabs.conf.js --grep @crossbrowser",
    "test:crossbrowser:report": "allure generate functional-output/cross-browser/reports/* -c -o functional-output/cross-browser/allure"
  },
