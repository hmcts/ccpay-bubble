    "js-money": "^0.6.3",
    "js-yaml": "^3.10.0",
    "jsdom": "^12.0.0",
    "mocha-junit-reporter": "^1.17.0",
    "mochawesome": "^3.0.2",
    "rxjs": "~6.3.3",
    "superagent": "3.8.3",
