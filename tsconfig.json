{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2022",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "target": "es2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2022",
      "dom"
    ],
    "paths": {
      "view-payment": [
        "dist/view-payment"
      ],
      "view-payment/*": [
        "dist/view-payment/*"
      ],
      "fee-register-search": [
        "dist/fee-register-search"
      ],
      "fee-register-search/*": [
        "dist/fee-register-search/*"
      ]
    }
  }
}
